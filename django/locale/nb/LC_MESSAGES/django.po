#: templates/routes/info.html:39
msgid "Operator"
msgstr "Ansvarlig"

#: templates/routes/info.html:45
msgid "Links"
msgstr "Lenker"

#: templates/routes/info.html:51
msgid "Sections"
msgstr "Deler"

